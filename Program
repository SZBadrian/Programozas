using System;

namespace Beadando
{
    class Program
    {
        static void Main(string[] args)
        {
            Random rnd = new Random();
            int randNum;
            randNum = rnd.Next(1, 100);
            int[] tomb = new int[10];
            for (int i = 0; i < tomb.Length; i++)

                tomb[i] = rnd.Next(1,100);
            Console.WriteLine("tomb.Length");
        }
    }
}
